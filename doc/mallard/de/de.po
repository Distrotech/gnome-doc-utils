msgid ""
"mal_block_p = element p { attribute style { xsd:NMTOKENS } ?, attribute * - "
"(mal:* | local:*) { text } *, <placeholder-1/> }"
msgstr ""
"mal_block_p = element p { attribute style { xsd:NMTOKENS } ?, attribute * - "
"(mal:* | local:*) { text } *, <placeholder-1/> }"

