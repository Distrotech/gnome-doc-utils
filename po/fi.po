msgid ""
"<msgid>appendix.label</msgid> <msgstr role='header'><number/>.&#x2003;</"
"msgstr> <msgstr role='li'><number/>.&#x2002;</msgstr> <msgstr>Appendix "
"<number/></msgstr>"
msgstr ""
