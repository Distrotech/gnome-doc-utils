msgid "format.chapter.label"
msgstr "Глава %t{number}"

#.
#. citetitle - The title of a cited work
#. http://docbook.org/tdg/en/html/citetitle.html
#.
#. This template formats an inline title citation.  This template uses
#. roles to specify the type of publication being cited, such as article
#. or book.
#.
#. Read this: ghelp:gnome-doc-xslt#translating
#. Then this: ghelp:gnome-doc-xslt#format.citetitle
#.
#: ../xslt/docbook/common/db-format.xml.in.h:42
msgid "format.citetitle"
msgstr ""
"<msg:msgstr xml:lang=\"bg\">\n"
"  <msg:msgstr role=\"article\">„%t{node}“</msg:msgstr>\n"
"  <msg:msgstr><i>%t{node}</i></msg:msgstr>\n"
"</msg:msgstr>"

#: ../xslt/docbook/common/db-format.xml.in.h:43
msgid "format.date"
msgstr ""
"<msg:msgstr xml:lang=\"bg\">\n"
"  <xsl:template>\n"
"    <xsl:value-of select=\"$day\"/>\n"
"    <xsl:text> </xsl:text>\n"
"    <xsl:choose>\n"
"      <xsl:when test=\"$month =  1\"><xsl:text>януари </xsl:text></xsl:"
"when>\n"
"      <xsl:when test=\"$month =  2\"><xsl:text>февруари </xsl:text></xsl:"
"when>\n"
"      <xsl:when test=\"$month =  3\"><xsl:text>март </xsl:text></xsl:when>\n"
"      <xsl:when test=\"$month =  4\"><xsl:text>април </xsl:text></xsl:when>\n"
"      <xsl:when test=\"$month =  5\"><xsl:text>май </xsl:text></xsl:when>\n"
"      <xsl:when test=\"$month =  6\"><xsl:text>юни </xsl:text></xsl:when>\n"
"      <xsl:when test=\"$month =  7\"><xsl:text>юли </xsl:text></xsl:when>\n"
"      <xsl:when test=\"$month =  8\"><xsl:text>август </xsl:text></xsl:"
"when>\n"
"      <xsl:when test=\"$month =  9\"><xsl:text>септември </xsl:text></xsl:"
"when>\n"
"      <xsl:when test=\"$month = 10\"><xsl:text>октомври </xsl:text></xsl:"
"when>\n"
"      <xsl:when test=\"$month = 11\"><xsl:text>ноември </xsl:text></xsl:"
"when>\n"
"      <xsl:when test=\"$month = 12\"><xsl:text>декември </xsl:text></xsl:"
"when>\n"
"    </xsl:choose>\n"
"    <xsl:value-of select=\"$year\"/>\n"
"  </xsl:template>\n"
"</msg:msgstr>"

